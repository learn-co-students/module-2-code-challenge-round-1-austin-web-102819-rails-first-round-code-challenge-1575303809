<%= form_for(@heroine) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br><br>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %> <br><br>

    <%= f.label :description %>
    <%= f.collection_select :power_id, Power.all, :id, :name %>

    <%= f.submit %>
<% end %>
  